#include <math.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int n;
    scanf("%d",&n);

    int a[n],i,c,k;
    for(i=1;i<=n;i++)
    {
        scanf("%d",&k);
        a[k]=i;
    }

    for(i=1;i<=n;i++)
    {
        printf("%d\n", a[a[i]]);
    }
    return 0;
}
